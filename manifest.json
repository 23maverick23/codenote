{
    "manifest_version": 2,
    "name": "Code Notes",
    "version": "1.0",
    "description": "Adds a fix editor to the bottom of a page.",
    "homepage_url": "http://ryancmorrissey.com/",
    "permissions": [
        "tabs",
        "*://*/*"
    ],
    "background": {
        "scripts": ["js/background.js"]
    },
    "browser_action": {
        "default_title": "CodeNotes",
        "default_icon": "icon_16x16.png"
    },
    "web_accessible_resources": [
        "js/*",
        "vendor/*",
        "css/*",
        "fonts/*"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}